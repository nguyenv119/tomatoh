{
  "manifest_version": 3,
  "name": "tomatoh",
  "description": "keeps you accountable",
  "version": "2.0.0",
  "permissions": ["storage", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Accomplishment Reminder"
  },
  "icons": {
    "128": "icons/tomatoh.png"
  }
}
